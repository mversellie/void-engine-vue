{
  "name": "void-engine-vue",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "jest": {
    "transform": {
      "^.+\\.vue$": "vue-jest",
      "^.+\\.js$": "babel-jest"
    },
    "verbose": true,
    "moduleNameMapper": {
      "^@[/](.+)": "<rootDir>/src/$1"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "vue"
    ]
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "axios": "^0.21.1",
    "bootstrap-vue": "^2.21.1",
    "jquery": "^3.5.1",
    "babel-core": "^7.0.0-bridge.0",
    "@popperjs/core": "^2.6.0",
    "portal-vue": "^2.1.7",
    "bootstrap": "^4.5.3",
    "sass-loader": "^10.1.0",
    "sass": "^1.30.0"
  },
  "devDependencies": {
    "vue-jest": "^3.0.7",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@testing-library/vue": "^5.6.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11",
    "jest": "^26.6.3",
    "@vue/babel-preset-app": "^4.5.9",
    "vue-axios-cors": "^1.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
